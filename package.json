    "scripts": {
        "build": "webpack --config ./bundler/webpack.prod.js",
        "dev": "concurrently \"npm run prettier-format\" \"npm run prettier-watch\" \"npm run fix\" \"npm run lint-watch\" \"webpack serve --config ./bundler/webpack.dev.js\"",
        "lint": "eslint '**/*.ts'",
        "lint-watch": "nodemon --exec 'npm run lint --silent'",
        "fix": "npm run lint -- --fix",
        "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}"
    },
    "dependencies": {
        "@babel/core": "^7.17.10",
        "@babel/preset-env": "^7.17.10",
        "babel-loader": "^8.2.5",
        "cannon": "^0.6.2",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^10.2.4",
        "css-loader": "^6.7.1",
        "dotenv": "^16.0.3",
        "file-loader": "^6.2.0",
        "gsap": "^3.11.4",
        "html-loader": "^3.1.0",
        "html-webpack-plugin": "^5.5.0",
        "lil-gui": "^0.18.0",
        "mini-css-extract-plugin": "^2.6.0",
        "portfinder-sync": "0.0.2",
        "raw-loader": "^4.0.2",
        "style-loader": "^3.3.1",
        "three": "^0.149.0",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.8.1",
        "webpack-merge": "^5.8.0"
    },
    "devDependencies": {
        "@types/cannon": "^0.1.8",
        "@types/three": "^0.149.0",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "concurrently": "^7.6.0",
        "eslint": "^8.34.0",
        "nodemon": "^2.0.20",
        "onchange": "^7.1.0",
        "prettier": "^2.8.4",
        "ts-loader": "^9.4.2",
        "typescript": "^4.9.4"
    }
}
